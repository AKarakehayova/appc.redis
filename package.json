    "grunt-contrib-jshint": "0.10.0",
    "grunt-mocha-cov": "0.4.0",
    "grunt-mocha-test": "0.12.0",
    "mocha": "1.21.4",
    "should": "4.0.4"
  },
  "scripts": {
    "coverage": "grunt coverage",
    "start": "node app",
    "test": "grunt"
